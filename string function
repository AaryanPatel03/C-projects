#include<stdio.h>
#include<string.h>
int main()
{
    char s1[]=" is a friend of ";
    char s2[2342];
    char s3[3243];
    char s4[3235];
    printf("Enter first name:\n");
    gets(s2);
    printf("Enter second name:\n");
    gets(s3);
    
    strcpy(s4,strcat(s2,s1));
    puts(strcat(s4,s3));


    return 0;
}
